{
  "name": "can-crud-app",
  "version": "0.9.0",
  "description": "A configureable data management app",
  "repository": "https://github.com/roemhildtgcan-crud-app",
  "watch": {
    "lint": "**/*.js",
    "test": "**/*.js"
  },
  "scripts": {
    "test": "testee test/index.html --reporter Spec --browsers firefox",
    "build": "steal-tools",
    "watch": "npm-watch",
    "docs": "documentjs --watch -d",
    "export": "node build.js",
    "lint": "jshint .",
    "pretest": "npm run lint"
  },
  "author": "roemhildtg",
  "license": "MIT",
  "devDependencies": {
    "documentjs": "^0.4.2",
    "jshint": "latest",
    "npm-watch": "^0.1.4",
    "steal": "^1.0.0-beta.3",
    "steal-tools": "^1.0.0-beta.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "can": "^2.3.23",
    "can-connect": "^0.5.x",
    "can-crud": "git+https://github.com/roemhildtg/can-crud.git",
    "can-restless": "latest",
    "can-ui": "git+https://github.com/roemhildtg/can-ui.git",
    "font-awesome": "^4.5.0",
    "jquery": "^2.2.3",
    "pubsub-js": "^1.5.3",
    "query-string": "^4.2.2"
  },
  "main": "can-crud-app/app/crud",
  "system": {
    "bundleAssets": true,
    "bundle": [
      "can-crud-app/config/default/default",
      "can-crud-app/config/default/Task",
      "can-crud-app/config/flask/flask",
      "can-crud-app/config/flask/Article",
      "can-crud-app/config/flask/Person_Basic",
      "can-crud-app/config/flask/Person_Advanced",
      "can-crud-app/config/flask/Visit"
    ],
    "npmIgnore": [
      "documentjs",
      "jshint",
      "steal-tools",
      "testee"
    ],
    "paths": {
      "bootstrap": "node_modules/bootstrap/dist/js/bootstrap.min.js"
    },
    "meta": {},
    "shim": {
      "jquery": {
        "exports": "jQuery"
      },
      "bootstrap": {
        "deps": [
          "jquery"
        ]
      }
    }
  }
}
